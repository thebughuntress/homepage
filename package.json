{
  "name": "homepage",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "check-git-status": "git diff-index --quiet HEAD -- || (echo 'Uncommitted changes detected. Please commit before building.' && exit 1)",
